{
  "name": "junit-processor",
  "version": "0.0.4",
  "description": "A JUnit XML post processing utility, for cleanup and similar purposes",
  "main": "index.js",
  "bin": "./bin/junit-processor",
  "scripts": {
    "ci": "npm run lint && npm run test",
    "test": "./node_modules/mocha/bin/mocha test/**/*.js --reporter spec",
    "lint": "eslint lib test",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitcoder/junit-processor.git"
  },
  "keywords": [
    "junit"
  ],
  "author": "Sergio Freire <sergio.freire@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bitcoder/junit-processor/issues"
  },
  "homepage": "https://github.com/bitcoder/junit-processor#readme",
  "dependencies": {
    "commander": "^8.3.0",
    "xml2js": "^0.4.23",
    "xmllint": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-xml": "^0.4.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0"
  }
}
